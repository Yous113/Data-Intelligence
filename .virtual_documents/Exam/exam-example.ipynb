def month_to_num(month):
    switcher = {
        "jan": 1,
        "feb": 2,
        "mar": 3,
        "apr": 4,
        "may": 5,
        "jun": 6,
        "jul": 7,
        "aug": 8,
        "sep": 9,
        "oct": 10,
        "nov": 11,
        "dec": 12
    }
    return switcher.get(month, 0)


df["month"].apply(month_to_num).hist(bins=12)
plt.xlabel("Month")
plt.ylabel("Count")
plt.title("Month Distribution for latest contact")
